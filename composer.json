{
	"type" : "project",
	"license" : "proprietary",
	"require" : {
		"php": "^7.1.3",
		"ext-ctype": "*",
		"ext-iconv": "*",
		"alcaeus/mongo-php-adapter": "^1.1",
		"doctrine/mongodb-odm-bundle": "^3.4",
		"kairos-project/api-doctrine-mongodb-odm-loader": "^1.0@dev",
		"kairos-project/api-pack": "^1.0@dev",
		"symfony/console": "^4.1",
		"symfony/flex": "^1.0",
		"symfony/framework-bundle": "^4.1",
		"symfony/lts": "^4@dev",
		"symfony/psr-http-message-bridge": "^1.0@dev",
		"symfony/serializer-pack": "dev-master",
		"symfony/yaml": "^4.1",
		"zendframework/zend-diactoros": "^1.8@dev"
	},
	"require-dev" : {
		"doctrine/doctrine-fixtures-bundle": "^3.0@dev",
		"symfony/browser-kit": "^4.1",
		"symfony/dotenv": "^4.1",
		"symfony/maker-bundle": "^1.0@dev",
		"symfony/phpunit-bridge": "^4.1",
		"phpunit/phpunit": "^7"
	},
	"config" : {
		"preferred-install" : {
			"*" : "dist"
		},
		"sort-packages" : true
	},
	"autoload" : {
		"psr-4" : {
			"App\\" : "src/"
		}
	},
	"autoload-dev" : {
		"psr-4" : {
			"App\\Tests\\" : "tests/"
		}
	},
	"replace" : {
		"symfony/polyfill-ctype" : "*",
		"symfony/polyfill-iconv" : "*",
		"symfony/polyfill-php71" : "*",
		"symfony/polyfill-php70" : "*",
		"symfony/polyfill-php56" : "*"
	},
	"conflict" : {
		"symfony/symfony" : "*"
	},
	"extra" : {
		"symfony" : {
			"allow-contrib" : false
		}
	},
	"minimum-stability" : "dev"
}
